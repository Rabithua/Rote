{
  "name": "rote_node",
  "version": "1.0.0",
  "description": "node | postgresql | prisma",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node server.ts ",
    "dev": "nodemon server.ts",
    "dev-f": "nodemon server.ts --ignore-errors",
    "dockerBuild": "docker buildx build --platform linux/amd64,linux/arm64 -t rabithua/rotebackend:latest --push .",
    "docker:start": "prisma generate && npm start",
    "test:init": "npx ts-node scripts/testInitialization.ts",
    "test:quick": "npx ts-node scripts/quickTest.ts",
    "test:all": "npm run test:quick && npm run test:init"
  },
  "keywords": [
    "node",
    "postgresql",
    "prisma"
  ],
  "author": "rabithua",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@prisma/client": "^6.19.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/formidable": "^3.4.6",
    "@types/node": "^24.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/web-push": "^3.6.4",
    "aws-sdk": "^2.1692.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "eslint": "^9.39.1",
    "express": "^5.1.0",
    "feed": "^5.1.0",
    "formidable": "^3.5.4",
    "jose": "^6.1.1",
    "moment": "^2.30.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "rate-limiter-flexible": "^8.2.1",
    "sharp": "^0.34.5",
    "web-push": "^3.6.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
