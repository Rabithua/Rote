{
  "name": "rote_node",
  "version": "1.0.0",
  "description": "node | postgresql | prisma",
  "main": "server.ts",
  "scripts": {
    "start": "bun run server.ts",
    "dev": "bun --watch server.ts",
    "dev-f": "bun --watch server.ts",
    "dockerBuild": "docker buildx build --platform linux/amd64,linux/arm64 -t rabithua/rotebackend:latest --push .",
    "docker:start": "prisma generate && bun run server.ts",
    "test:init": "bun run scripts/testInitialization.ts",
    "test:quick": "bun run scripts/quickTest.ts",
    "test:all": "bun run test:quick && bun run test:init"
  },
  "keywords": [
    "node",
    "postgresql",
    "prisma"
  ],
  "author": "rabithua",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@prisma/client": "^6.19.0",
    "@types/node": "^24.10.1",
    "@types/web-push": "^3.6.4",
    "aws-sdk": "^2.1692.0",
    "eslint": "^9.39.1",
    "feed": "^5.1.0",
    "hono": "^4.6.0",
    "jose": "^6.1.1",
    "moment": "^2.30.1",
    "rate-limiter-flexible": "^8.2.1",
    "web-push": "^3.6.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "sharp": "^0.34.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
