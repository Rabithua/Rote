{
  "name": "prisma_node_practice",
  "version": "1.0.0",
  "description": "prisma 的一个小练习",
  "main": "index.js",
  "scripts": {
    "start": "npx ts-node server.ts ",
    "dev": "nodemon server.ts",
    "dev-f": "nodemon server.ts --ignore-errors",
    "mongoInit": "docker-compose up -d",
    "mongoSet": "docker exec -it RoteMongo1 mongosh --eval \"rs.initiate({_id: 'myReplicaSet', members: [{_id: 0, host: 'RoteMongo1'}, {_id: 1, host: 'RoteMongo2'}]})\" && docker exec -it RoteMongo1 mongo --eval \"use Rote\" && docker exec -it RoteMongo1 mongo --eval \"db.createCollection('Rote')\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.9.1",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/express": "^4.17.21",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "moment": "^2.29.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  }
}
