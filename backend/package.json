{
  "name": "rote_node",
  "version": "1.0.0",
  "description": "node | mongodb | prisma",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node server.ts ",
    "dev": "nodemon server.ts",
    "dev-f": "nodemon server.ts --ignore-errors",
    "dbSchemaUpdate": "npx prisma db push && npx prisma generate",
    "dockerBuild": "docker buildx build --platform linux/amd64,linux/arm64 -t rabithua/rotebackend:latest --push .",
    "docker:start": "prisma generate && npm start"
  },
  "keywords": [
    "node",
    "mongodb",
    "prisma"
  ],
  "author": "rabithua",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.484.0",
    "@aws-sdk/s3-request-presigner": "^3.484.0",
    "@hokify/agenda": "^6.3.0",
    "@prisma/client": "^6.10.0",
    "@rabithua/prisma-session-store": "^1.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.9.1",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/web-push": "^3.6.3",
    "aws-sdk": "^2.1528.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "feed": "^5.0.1",
    "formidable": "^3.5.1",
    "moment": "^2.29.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "rate-limiter-flexible": "^4.0.1",
    "sharp": "^0.32.6",
    "web-push": "^3.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "prisma": "^6.10.0",
    "ts-node": "^10.9.2"
  }
}